{
  "parser": "babel-eslint",
  "env": {
    "es6": true,
    "browser": true,
    "node": true
  },
  "rules": {
    "semi": "off",
    "arrow-parens": "off",
    "object-curly-spacing": ["error", "always"],
    "require-jsdoc": "off",
    "no-invalid-this": "off"
  },
  "extends": ["eslint:recommended", "google"]
}
